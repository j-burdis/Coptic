<div class="mb-12">
  <div class="mt-1 mb-12 grid grid-cols-1 md:grid-cols-2 gap-4 items-start">
    <%= render 'search_form',
        form_url: gallery_design_path,
        show_type_filter: false,
        show_subcategory_filter: true,
        quote: @design_quote %>
  </div>

  <%= render 'subcategory_grid',
      subcategories: @design_subcategory_pages,
      fallback_subcategories: @design_subcategories,
      show_year: true %>
</div>
